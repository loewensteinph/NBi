<?xml version="1.0" encoding="utf-8" ?>
<testSuite name="Acceptance Testing: query equalTo Csv" xmlns="http://NBi/TestSuite">
	<settings>
		<default apply-to="system-under-test">
		  <connectionString>Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=master;Integrated Security=True</connectionString>
		</default>
	</settings>
	<test name="Excel Test" uid="0001">
		<system-under-test>
			<execution>
        <query>
          SELECT TOP 5
          Test = '01.01.1980',
          object_id,
          schema_id
          FROM master.sys.objects
          ORDER BY object_id;
        </query>
			</execution>
		</system-under-test>
		<assert>
			<equalTo>
        <resultSet file="..\Excel\Sample.xlsx" sheet="Assert1">
        </resultSet>
			</equalTo>
		</assert>
	</test>
</testSuite>
